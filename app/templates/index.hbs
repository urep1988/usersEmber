<div class="container" style="background-color: #B0C4DE; height:100vh;">
	<h1 align="center">Users information</h1>
	<div class="col-sm-2"></div>
	<div class="col-sm-8">
		<table class="table table-hover table-striped">
			<thead>
				<tr>
					<th>Name</th>
					<th>Age</th>
					<th>Sex</th>
					<th>Email</th>
					<th>Detail information</th>
					<th>Photo</th>
					<th></th>
				</tr>
			</thead>
			<tbody>
				{{#each model as |userListUnit|}}
					<tr>
						<td>{{userListUnit.name}}</td>
						<td>{{userListUnit.age}}</td>
						<td>{{userListUnit.sex}}</td>
						<td>{{userListUnit.email}}</td>
						<td>{{#link-to "about" userListUnit.id}}{{userListUnit.about}}{{/link-to}}</td>
						<td>{{user-listing userList=userListUnit}}</td>
						<td id="friend" class="{{userListUnit.id}}">{{friend-listing friend=userListUnit}}</td>
					</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
</div>
